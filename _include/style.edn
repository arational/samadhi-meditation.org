(list
 [:body {:margin 0
         :font-family :Sans-Serif
         :background-color :#eee
         :font-size (u/vw 1.8)
         :font-weight 300
         :color :#333}]
 [:a {:color :#f45300
      :text-decoration :none
      :transition "color .25s ease-in-out"
      :-moz-transition "color .25s ease-in-out"
      :-webkit-transition "color .25s ease-in-out"}
  [:&:hover {:color :#001857}]]
 [:.prim {:padding 0
          :margin 0
          :text-decoration :underline
          :font-size (u/vw 3)
          :text-align :center
          :font-weight :bold
          :color :#e51d00
          :display :block}]
 [:.sec {:text-decoration :none
         :text-align :center
         :font-size (u/vw 1.5)
         :display :block}]
 [:h2 {:color :#fff
       :background-color :#984b00
       :opacity 0.8
       :padding (u/em 0.5)
       :margin 0
       :font-weight 400
       :font-size (u/em 1.2)
       :letter-spacing (u/px 3)
       :display :flex
       :justify-content :space-between
       :align-items :center}
  [:a {:color :#fff}]]
 [:h3 {:margin 0
       :padding (u/em 0.5)
       :padding-bottom 0
       :color :#572b00
       :font-weight 400}]
 [:h4 {:margin 0
       :padding (u/em 0.5)
       :padding-bottom 0
       :font-weight :bold}]
 [:p {:margin-left (u/em 1)
      :margin-right (u/em 1)
      :line-height (u/em 1.5)}]
 [:ul {:margin-left 0
       :margin-right 0
       :margin-bottom (u/em 1)
       :margin-top (u/em 1)
       :padding-left (u/em 1)
       :list-style-type :circle}
  [:li {:margin-left (u/em 1)
        :line-height (u/em 1.5)}]]
 [:header {:display :flex
           :height (u/em 3)}]
 [:section {:display :flex
            :flex-direction :column}]
 [:footer {:background-color :#ddd
           :padding-top (u/em 1)
           :padding-bottom (u/em 1)
           :text-align :center}]
 [:#slider {:background-size "100% auto"
            :background-repeat :no-repeat
            :display :flex
            :flex-direction :column
            :justify-content :space-between}]
 [:.menu-toggle {:display :none}]
 [:.toprow {:display :flex
            :justify-content :space-between}]
 [:.logo {:width (u/vw 10)
          :padding (u/em 0.5)
          :opacity 0.7
          :height (u/vw 12)}]
 [:.menu {:padding 0
          :margin 0
          :list-style :none
          :width (u/percent 100)
          :display :flex
          :justify-content :space-around
          :align-items :center
          :color :#984b00
          :font-size (u/vw 2)}
  [:li {:margin 0}]
  [:a {:padding (u/em 0.5)}]
  [:.current [:a {:color :#001857}]]]
 [:.progress {:padding 0
              :margin 0
              :list-style :none
              :padding-top (u/em 0.5)
              :padding-bottom (u/em 0.5)
              :opacity 0.7
              :color :#fff
              :display :none
              :flex-direction :column
              :text-align :right}
  [:li {:padding-right (u/em 1.5)}]
  [:.current {:color :#f45300}]]
 [:.submenu {:padding 0
             :margin 0
             :list-style :none
             :padding-top (u/em 0.5)
             :padding-bottom (u/em 0.5)
             :text-align :right
             :color :#fff
             :opacity 0.7
             :font-weight 400
             :font-size (u/vw 2)}
  [:li {:padding (u/em 0.5)
        :padding-right (u/em 1)}]
  [:a {:color :#fff}
   [:&:hover {:color :#001857}]]]
 [:.picture {:width (u/percent 50)
             :float :right
             :margin-left (u/em 1)
             :margin-bottom (u/em 1)}]
 [:.picture-right {:float :right
                   :width (u/percent 50)
                   :margin-left (u/em 1)
                   :margin-bottom (u/em 1)}]
 [:.picture-left {:float :left
                  :width (u/percent 50)
                  :margin-right (u/em 1)
                  :margin-bottom (u/em 1)}]
 [:.icon {:color :blue
          :width (u/vw 5)
          :float :left
          :margin-right (u/em 1)
          :margin-bottom (u/em 1)}]
 [:.subtitle {:font-weight :bold
              :display :block
              :color :#888
              :padding-bottom (u/em 1)}]
 [:.special {:background-color :#d0d0f0}]
 [:.regular {:background-color :#d0f0d0}]
 (at-media
  {:max-width (u/px 480)}
  [:body {:font-size (u/vw 3.5)}]
  [:header {:height (u/px 52)}]
  [:.picture {:width (u/percent 100)
              :margin 0
              :float :none}]
  [:.picture-left :.picture-right {:display :none}]
  [:.progress {:display :inline-block}]
  [:.submenu {:display :none}]
  [:.caption {:background-size "100% auto"
              :display :flex
              :flex-direction :column
              :justify-content :space-between}]
  [:.menu-toggle {:width (u/em 4)
                  :display :inline
                  :border :none
                  :cursor :pointer
                  :background-color :transparent
                  :padding (u/em 2)
                  :background-image (url "images/menu.svg")
                  :background-repeat :no-repeat
                  :background-position "center center"}]
  [:.menu {:top (u/px 75)
           :left 0
           :display :none
           :position :absolute
           :background-color :#eee
           :width (u/px 200)
           :box-shadow "0px 8px 16px 0px rgba(0,0,0,0.2)"
           :z-index 1
           :font-size (u/vw 3)}
   [:li :a {:padding (u/em 1)}]]
  [:.show {:display :block}]
  [:.video {:width (u/percent 100)}]
  [:.icon {:width (u/vw 15)}])
 (at-keyframes
  :fadeIn
  [:0% {:opacity 0}]
  [:100% {:opacity 0.7
          :transition "opacity .25s ease-in-out"
          :-moz-transition "opacity .25s ease-in-out"
          :-webkit-transition "opacity .25s ease-in-out"}])
 [:.fade-in {:-webkit-animation-name :fadeIn
             :animation-name :fadeIn
             :animation-duration (u/s 2)}])
  ;; calendar
  [:table.calendar {:width (u/percent 100)}]
  [:.calendar-list {:position :relative
                    :overflow :hidden
                    :margin-left (u/em 1)
                    :margin-right (u/em 1)
                    :height (u/vw 70)}
   [:.calendar-item {:position :absolute
                     :top 0
                     :width (u/percent 100)
                     :height (u/percent 100)
                     :overflow :hidden
                     :margin 0}]
   [(s/+ :input.switcher:checked :.calendar-item)
    [:.switch-controller {:opacity 1}]]
   [:.switch-controller {:position :absolute
                         :left 0
                         :top 0
                         :z-index 1
                         :width (u/percent 100)}]
   [:label.prev :label.next {:cursor :pointer
                             :position :relative
                             :top 0
                             :display :block
                             :line-height (u/em 2)
                             :font-size (u/em 1.5)}]
   [:label.prev {:float :left}]
   [:label.next {:float :right}]]
  (do (s/defclass switcher)
      [(s/+ (s/selector (s/input (switcher (s/not s/checked)))) :.calendar-item)
       {:-webkit-transform "translateX(-100vw)"
        :-ms-transform "translateX(-100vw)"
        :transform "translateX(-100vw)"
        :-webkit-transition "-webkit-transform 0.4s ease"
        :transition "transform 0.4s ease"}])
  [(s/- :input.switcher:checked :.calendar-item)
   {:-webkit-transform "translateX(100vw)"
    :-ms-transform "translateX(100vw)"
    :transform "translateX(100vw)"}]
  [(s/+ :input.switcher:checked :.calendar-item)
   {:-webkit-transform "translateX(0px)"
    :-ms-transform "translateX(0px)"
    :transform "translateX(0px)"
    :-webkit-transition "-webkit-transform 0.4s ease"
    :transition "transform 0.4s ease"}]
  [(s/+ :input.switcher:checked :.calendar-item)
   [:.switch-controller {:-webkit-transition "opacity 1s ease 0.4s"
                         :transition "opacity 1s ease 0.4s"}]]))
